{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "dialog:allow-message",
    "dialog:allow-confirm",
    "dialog:allow-ask",
    "dialog:allow-open",

    "sql:default",
    "sql:allow-load",
    "sql:allow-select",
    "sql:allow-execute",
    "sql:allow-close"
  ]
}
